try:
    num1 = int(input('첫 번째 숫자를 입력하시오: '))
    num2 = int(input('두 번째 숫자를 입력하시오: '))
    t = input('연산자(+,-,*,/): ')
    if t == '+':
        result = num1 + num2
    elif t == '-':
        result = num1 - num2
    elif t == '*':
        result = num1 * num2
    elif t == '/':
        result = num1 / num2

except ValueError:
    print('숫자만 입력 가능합니다.')
except ZeroDivisionError:
    print('0으로 나눌 수 없습니다.')
else: 
    print('결과: ',result)
finally:
    print('종료')




   